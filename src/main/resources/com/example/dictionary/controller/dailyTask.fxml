<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.geometry.Insets?>

<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml" alignment="CENTER" maxHeight="100.0" maxWidth="1.7976931348623157E308"
      minHeight="87.0" prefHeight="87.0" prefWidth="379.0" spacing="10.0">
    <children>
        <ImageView fx:id="img" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true"/>
        <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" spacing="10.0" HBox.hgrow="ALWAYS">
            <children>
                <HBox>
                    <children>
                        <Label fx:id="name" text="Thêm mới 5 từ"/>
                        <HBox alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
                            <children>
                                <Label fx:id="detail" text="1/3"/>
                            </children>
                        </HBox>
                    </children>
                </HBox>
                <ProgressBar fx:id="bar" maxWidth="1.7976931348623157E308" prefWidth="200.0" progress="0.0"/>
                <HBox alignment="CENTER">
                    <children>
                        <Label fx:id="gem" text="x5"/>
                        <ImageView fx:id="gemImg" fitHeight="16.0" fitWidth="17.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <HBox.margin>
                                <Insets left="5.0"/>
                            </HBox.margin>
                        </ImageView>
                        <HBox alignment="CENTER_RIGHT" prefHeight="0.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                            <children>
                                <Label fx:id="btn" alignment="CENTER" prefHeight="18.0" prefWidth="70.0"
                                       styleClass="btn-2" text="Thực hiện"/>
                            </children>
                        </HBox>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</HBox>
